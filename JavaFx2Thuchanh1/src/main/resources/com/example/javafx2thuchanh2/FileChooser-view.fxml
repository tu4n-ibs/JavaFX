<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.ImageView?>
<AnchorPane fx:id="ap" prefHeight="400" prefWidth="600"
            xmlns="http://java.sun.com/JSP/Page" xmlns:fx="http://java.sun.com/JSP/Page"
            fx:controller="com.example.javafx2thuchanh2.FileChooserController">
  <children>
      <ImageView fx:id="imageView" fitWidth="396" layoutX="100" layoutY="50"
                 pickOnBounds="true" preserveRatio="true" />

      <Button layoutX="256" layoutY="300" mnemonicParsing="false"
              onAction="#chooseImage" text="Open" alignment="CENTER"/>
  </children>
</AnchorPane>
