<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.csclient.HelloController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <ScrollPane fx:id="scrollPane" prefWidth="350" prefHeight="230" layoutX="20" layoutY="10">
            <content>
                <VBox fx:id="vbox" prefWidth="340" prefHeight="170"/>
            </content>
        </ScrollPane>
            <Button text="Enter" fx:id="button" layoutX="320" layoutY="250"/>
            <TextField fx:id="textField" layoutX="20" layoutY="250" prefWidth="290"/>
    </children>
</AnchorPane>
